  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'fusshuhn_32x32.jpg' %>

  <!-- link rel="icon" type="image/jpeg" href="/assets/images/fusshuhn_32x32.jpg" sizes="32x32" --><!--fav-->
